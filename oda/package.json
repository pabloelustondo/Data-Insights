{
  "name": "ODA",
  "version": "0.0.1",
  "description": "This service provides and API to query SOTI data analytics service",
  "scripts": {
    "start": "npm run build && node ./dist/server.js",
    "startdebug": "npm run build && node %NODE_DEBUG_OPTION% ./dist/server.js",
    "startmac": "npm run build && node $NODE_DEBUG_OPTION ./dist/server.js",
    "build": "npm run clean && npm run lint && npm run swagger-gen && npm run routes-gen && tsc && npm run copy-swagger-ui",
    "lint": "tslint ./src/**/*.ts",
    "clean": "rimraf dist",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "copy-swagger-ui": "ncp ./src/swagger-ui ./dist/swagger-ui",
    "tsoa": "tsoa",
    "install-windows-service": "winser -i",
    "uninstall-windows-service": "winser -r",
    "dev-test-watch": "mocha-typescript-watch",
    "test": "tsc -p . "
  },
  "author": "pablo.elustondo@soti.net",
  "license": "MIT",
  "dependencies": {
    "@types/request": "0.0.36",
    "@types/request-promise": "4.1.33",
    "@types/socket.io": "^1.4.29",
    "aws-sdk": "2.9.0",
    "body-parser": "1.15.2",
    "chai": "3.5.0",
    "dynalite": "1.1.0",
    "express": "4.14.0",
    "express-winston": "2.1.2",
    "helmet": "3.4.1",
    "jsonwebtoken": "7.2.1",
    "local-dynamo": "0.4.1",
    "method-override": "2.3.6",
    "mocha-typescript": "1.0.11",
    "querystring": "0.2.0",
    "request": "2.79.0",
    "request-promise": "4.1.1",
    "socket.io": "^2.0.1",
    "tsoa": "1.0.14",
    "winser": "1.0.2",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.30",
    "@types/express": "4.0.30",
    "@types/method-override": "0.0.27",
    "@types/node": "6.0.33",
    "@types/winston": "0.0.32",
    "ncp": "2.0.0",
    "rimraf": "2.5.2",
    "tslint": "3.11.0",
    "typescript": "2.1.4",
    "mocha": "3.2.0",
    "describe": "1.2.0",
    "chai-http": "3.0.0",
    "minimatch": "3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "http://taipan:8080/tfs/SOTITFS/_git/CustomerBI"
  }
}
